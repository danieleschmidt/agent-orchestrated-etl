{
  "name": "agent-orchestrated-etl",
  "version": "0.0.1",
  "description": "Hybrid Airflow + LangChain project where an agent decides which ETL step or retriever to trigger",
  "scripts": {
    "test": "python -m pytest tests/ -v",
    "test:watch": "python -m pytest tests/ -v --tb=short -f",
    "test:coverage": "python -m coverage run -m pytest tests/ && python -m coverage report -m",
    "test:integration": "python -m pytest tests/ -v -m integration",
    "lint": "python -m ruff check src/ tests/",
    "lint:fix": "python -m ruff check --fix src/ tests/",
    "format": "python -m black src/ tests/",
    "format:check": "python -m black --check src/ tests/",
    "typecheck": "python -m mypy src/",
    "security": "python -m bandit -r src/",
    "quality": "python -m radon cc src/agent_orchestrated_etl -s -a",
    "secrets": "detect-secrets scan --all-files --baseline .secrets.baseline",
    "pre-commit": "pre-commit run --all-files",
    "build": "python -m build",
    "clean": "rm -rf build/ dist/ *.egg-info/ .pytest_cache/ .mypy_cache/ .ruff_cache/ .coverage htmlcov/",
    "dev": "python -m agent_orchestrated_etl.cli --help",
    "dev:api": "bash scripts/dev/run-api.sh",
    "dev:test": "bash scripts/dev/run-tests.sh", 
    "dev:lint": "bash scripts/dev/lint.sh",
    "dev:format": "bash scripts/dev/format.sh",
    "install": "pip install -e .[dev]",
    "install:prod": "pip install -e .",
    "docs:build": "mkdocs build",
    "docs:serve": "mkdocs serve",
    "docker:build": "docker build -t agent-orchestrated-etl .",
    "docker:run": "docker run -it --rm agent-orchestrated-etl",
    "docker:compose": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "validate": "npm run lint && npm run typecheck && npm run test && npm run security",
    "sbom:generate": "python scripts/generate_sbom.py",
    "security:scan": "python scripts/vulnerability_scanner.py",
    "performance:benchmark": "python scripts/performance_benchmark.py",
    "chaos:test": "python chaos-engineering/chaos-runner.py",
    "infra:plan": "cd infrastructure/terraform && terraform plan",
    "infra:apply": "cd infrastructure/terraform && terraform apply",
    "k8s:deploy": "kubectl apply -f infrastructure/kubernetes/",
    "optimization:full": "npm run sbom:generate && npm run security:scan && npm run performance:benchmark"
  },
  "keywords": [
    "etl",
    "airflow",
    "langchain",
    "data-engineering",
    "orchestration",
    "agents",
    "ai",
    "data-pipelines"
  ],
  "author": "Terragon Labs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/agent-orchestrated-etl.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/agent-orchestrated-etl/issues"
  },
  "homepage": "https://github.com/your-org/agent-orchestrated-etl#readme"
}