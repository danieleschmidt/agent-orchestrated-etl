# Agent-Orchestrated ETL Environment Configuration

# =============================================================================
# Core Application Settings
# =============================================================================
DEBUG=false
LOG_LEVEL=INFO
ENVIRONMENT=development

# =============================================================================
# Database Configuration
# =============================================================================
DATABASE_URL=postgresql://user:password@localhost:5432/etl_db
DATABASE_POOL_SIZE=10
DATABASE_MAX_OVERFLOW=20

# =============================================================================
# Airflow Configuration
# =============================================================================
AIRFLOW_HOME=./airflow
AIRFLOW__CORE__DAGS_FOLDER=./dags
AIRFLOW__CORE__EXECUTOR=LocalExecutor
AIRFLOW__CORE__SQL_ALCHEMY_CONN=postgresql://airflow:airflow@localhost:5432/airflow
AIRFLOW__CORE__LOAD_EXAMPLES=false
AIRFLOW__WEBSERVER__EXPOSE_CONFIG=false
AIRFLOW__WEBSERVER__RBAC=true

# =============================================================================
# AWS Configuration
# =============================================================================
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
AWS_SECRETS_MANAGER_REGION=us-east-1

# Specific AWS Services
S3_BUCKET_NAME=your-etl-bucket
S3_PREFIX=data/
REDSHIFT_CLUSTER_ENDPOINT=your-cluster.redshift.amazonaws.com
REDSHIFT_DATABASE=dev
REDSHIFT_PORT=5439

# =============================================================================
# LangChain & AI Configuration
# =============================================================================
OPENAI_API_KEY=your_openai_api_key_here
LANGCHAIN_TRACING_V2=true
LANGCHAIN_API_KEY=your_langchain_api_key_here
LANGCHAIN_PROJECT=agent-orchestrated-etl

# Agent-specific settings
AGENT_MAX_ITERATIONS=50
AGENT_TEMPERATURE=0.1
AGENT_MODEL=gpt-4
AGENT_TIMEOUT_SECONDS=300

# =============================================================================
# Monitoring & Observability
# =============================================================================
PROMETHEUS_PORT=9090
GRAFANA_PORT=3000
MONITORING_WEBSOCKET_PORT=5555
ENABLE_METRICS=true
METRICS_ENDPOINT=/metrics

# Application Performance Monitoring
APM_SERVICE_NAME=agent-orchestrated-etl
APM_ENVIRONMENT=development
JAEGER_ENDPOINT=http://localhost:14268/api/traces

# =============================================================================
# Security Configuration
# =============================================================================
SECRET_KEY=your-secret-key-change-in-production
JWT_SECRET_KEY=your-jwt-secret-key-change-in-production
JWT_EXPIRATION_HOURS=24

# SSL/TLS Configuration
SSL_ENABLED=false
SSL_CERT_PATH=/path/to/cert.pem
SSL_KEY_PATH=/path/to/key.pem

# =============================================================================
# Data Source Connections
# =============================================================================

# PostgreSQL
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_postgres_password
POSTGRES_DATABASE=etl_source

# MySQL
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_USER=mysql
MYSQL_PASSWORD=your_mysql_password
MYSQL_DATABASE=etl_source

# MongoDB
MONGODB_CONNECTION_STRING=mongodb://localhost:27017/etl_source
MONGODB_DATABASE=etl_source

# Redis (for caching and queuing)
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password
REDIS_DB=0

# =============================================================================
# API Configuration
# =============================================================================
API_HOST=0.0.0.0
API_PORT=8000
API_WORKERS=4
API_TIMEOUT=60
API_RATE_LIMIT=1000/hour

# External API Keys
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/SLACK/WEBHOOK
TEAMS_WEBHOOK_URL=https://your-teams-webhook-url
EMAIL_SMTP_HOST=smtp.gmail.com
EMAIL_SMTP_PORT=587
EMAIL_USERNAME=your_email@example.com
EMAIL_PASSWORD=your_email_password

# =============================================================================
# Testing Configuration
# =============================================================================
TEST_DATABASE_URL=postgresql://test:test@localhost:5432/test_etl_db
TEST_S3_BUCKET=test-etl-bucket
TEST_REDIS_DB=1

# =============================================================================
# Development Tools
# =============================================================================
ENABLE_DEBUG_TOOLBAR=true
ENABLE_PROFILING=false
HOT_RELOAD=true

# Pre-commit hooks
PRE_COMMIT_ENABLED=true

# =============================================================================
# Container Configuration
# =============================================================================
DOCKER_REGISTRY=your-registry.com
DOCKER_TAG=latest
CONTAINER_MEMORY_LIMIT=2g
CONTAINER_CPU_LIMIT=1

# =============================================================================
# Feature Flags
# =============================================================================
ENABLE_VECTOR_SEARCH=false
ENABLE_ADVANCED_MONITORING=true
ENABLE_EXPERIMENTAL_FEATURES=false
ENABLE_AGENT_LEARNING=true
ENABLE_AUTO_SCALING=false

# =============================================================================
# Backup & Recovery
# =============================================================================
BACKUP_ENABLED=true
BACKUP_SCHEDULE=0 2 * * *
BACKUP_S3_BUCKET=your-backup-bucket
BACKUP_RETENTION_DAYS=30